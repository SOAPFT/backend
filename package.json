{
  "name": "soapft-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.js --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "test:cov:all": "jest --coverage && jest --config ./test/jest-integration.json --coverage && jest --config ./test/jest-e2e.json --coverage",
    "typeorm": "cross-env NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js --dataSource ./config/orm.config.ts",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:up": "docker-compose -f docker-compose.dev.yml up --build",
    "migration:create": "cross-env NODE_ENV=development npm run typeorm migration:create",
    "migration:create:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm/cli.js -d dist/config/orm.config.js migration:create",
    "migration:generate": "cross-env NODE_ENV=development npm run typeorm migration:generate",
    "migration:generate:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm/cli.js -d dist/config/orm.config.js migration:generate",
    "migration:run": "cross-env NODE_ENV=development npm run typeorm migration:run",
    "migration:run:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm/cli.js -d dist/config/orm.config.js migration:run",
    "migration:revert": "cross-env NODE_ENV=development npm run typeorm migration:revert",
    "migration:revert:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm/cli.js -d dist/config/orm.config.js migration:revert",
    "migration:show": "cross-env NODE_ENV=development npm run typeorm migration:show",
    "migration:show:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm/cli.js -d dist/config/orm.config.js migration:show",
    "db:seed": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d config/orm.config.ts",
    "db:seed:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d dist/config/orm.config.js",
    "db:create": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs db:create -d config/orm.config.ts",
    "db:create:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm-extension/bin/cli.cjs db:create -d dist/config/orm.config.js",
    "db:drop": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs db:drop -d config/orm.config.ts",
    "db:drop:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm-extension/bin/cli.cjs db:drop -d dist/config/orm.config.js",
    "db:sync": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./scripts/db-sync.ts",
    "db:sync:prod": "cross-env NODE_ENV=production node dist/scripts/db-sync.js",
    "db:refresh": "npm run db:drop && npm run db:create && npm run db:sync && npm run db:seed",
    "db:refresh:prod": "npm run db:drop:prod && npm run db:create:prod && npm run db:sync:prod && npm run db:seed:prod"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.844.0",
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/client-sqs": "^3.879.0",
    "@aws-sdk/lib-storage": "^3.844.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
