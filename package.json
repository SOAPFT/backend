    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "install": "^0.13.0",
    "jwks-rsa": "^3.2.0",
    "ms": "^2.1.3",
    "multer": "^2.0.1",
    "nanoid": "^3.3.4",
    "nest-winston": "^1.10.2",
    "node-apn": "^3.0.0",
    "npm": "^11.4.2",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-naver": "^1.0.6",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "starving-orange": "^1.1.5",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.21",
    "typeorm-extension": "^3.7.0",
    "ulid": "^3.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "run-script-webpack-plugin": "^0.2.2",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "stream-browserify": "^3.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
